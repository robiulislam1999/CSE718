      ],
      "source": [
        "from sympy import symbols, Eq, solve\n",
        "T, a, alpha, I, m, g, r = symbols('T a alpha I m g r')\n",
        "eq1 = Eq(a, -r * alpha)\n",
        "eq2 = Eq(T - m*g, m * a)\n",
        "eq3 = Eq(T * r, I * alpha)\n",
        "soln = solve([eq1, eq2, eq3], [T, a, alpha])\n",
        "soln"
      ]
